upstream phpfcgi {
    server 127.0.0.1:9000;
    # server unix:/var/run/php5-fpm.sock; #for PHP-FPM running on UNIX socket
}

server {
    listen 80;
    listen 443;

    server_name blackhole;

    root        /srv/www/htdocs/blackhole;

    error_log   /var/log/nginx/blackhole.error.log;
    access_log  /var/log/nginx/blackhole.access.log;

    location / {
    }
}
